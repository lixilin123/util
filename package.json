    "prebuild": "typedoc src/ --out public --excludePrivate --excludeProtected "
  },
  "private": true,
  "devDependencies": {
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
