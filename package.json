    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.4.3"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lixilin123/util.git"
  }
