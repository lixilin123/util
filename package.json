  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "promise": "^8.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "watch:ts": "tsc -w",
    "watch:scss": "node-sass -w ./src -o ./lib",
